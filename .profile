export GRADLE_HOME="${HOME}/gradle"
export GROOVY_HOME="${HOME}/groovy"
export MAVEN_HOME="${HOME}/maven"
export ANDROID_HOME="${HOME}/android-sdk"
export ANDROID_SDK_ROOT="${ANDROID_HOME}"
export ANDROID_NDK_HOME="${ANDROID_HOME}/ndk-bundle"
export ANDROID_NDK="${ANDROID_NDK_HOME}"
export GOROOT="/usr/lib/go"
export GOPATH="${HOME}/gocode"

export JAVA_HOME=/usr/lib/jvm/default-java
export WINEARCH=win32
export WINEPREFIX=${HOME}/.wine32
export QEMU_AUDIO_DRV="sdl"
export GPG_TTY=$(tty)
export VISUAL="${HOME}/bin/emacs"
export EDITOR="${HOME}/bin/emacs"

export SLS_DEBUG="*"

export DICPATH="/usr/share/hunspell"
export GTK_CSD=0
export LD_PRELOAD=/usr/lib/x86_64-linux-gnu/libgtk3-nocsd.so.0

export PROJECT_HOME="$HOME/dev"
export WORKON_HOME="$HOME/.virtualenvs"
export VIRTUALENVWRAPPER_PYTHON=/usr/bin/python3
export VIRTUALENVWRAPPER_VIRTUALENV=/usr/bin/virtualenv

export PATH="$GRADLE_HOME/bin:$GROOVY_HOME/bin:$HOME/ideaIC/bin:/usr/sbin:${ANDROID_HOME}/platform-tools:${ANDROID_HOME}/tools/bin:$HOME/eclipse:$MAVEN_HOME/bin:$HOME/.local/bin:${HOME}/bin:${HOME}/.local/crosstool-ng/bin:${GOROOT}/bin:${GOPATH}/bin:${HOME}/.luarocks/bin:/usr/local/openresty/bin:${HOME}/.cabal/bin:$HOME/.cargo/bin:$PATH"

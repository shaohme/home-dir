Xcursor.theme: Adwaita
Xcursor.size: 32
Xft.antialias: 1
Xft.autohint: 0
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! *font: xft:Monospace:size=32:antialias=true,xft:Symbola:pixelsize=26:antialias=true
! *font: -*-terminus-*-*-*-*-32-*-*-*-*-*-*-*
! *font: -*-*-*-*-*-*-32-*-*-*-*-*-*-*

! URxvt*font: -*-terminus-bold-*-*-*-28-*-*-*-*-*-*-*
! XTerm*font: -*-terminus-bold-*-*-*-28-*-*-*-*-*-*-*
XTerm*font:     xft:Monospace:size=14:antialias=true,xft:Symbola:pixelsize=10:antialias=true
XTerm*boldFont: xft:Monospace-bold:bold:size=14:antialias=true,xft:Symbola:pixelsize=10:antialias=true
XTerm*background:  #1c1b1a
XTerm*foreground:  #f8f6f2
XTerm*cursorColor: #aeee00
XTerm*scrollColor: #45413b

Emacs.font: monospace 14

!! Defining italic and bold fonts because they look like crap
!! in X with terminus font
! *boldFont: -*-terminus-bold-*-*-*-28-*-*-*-*-*-*-*
! *italicFont: -*-terminus-bold-*-*-*-28-*-*-*-*-*-*-*
! *bolditalicFont: -*-terminus-bold-*-*-*-28-*-*-*-*-*-*-*
URxvt*font:     xft:Monospace:size=14:antialias=true,xft:Symbola:pixelsize=10:antialias=true
URxvt*boldFont: xft:Monospace-bold:bold:size=14:antialias=true,xft:Symbola:pixelsize=10:antialias=true
! URxvt*font:     xft:Terminus:bold:size=26,xft:Symbola:pixelsize=19
! URxvt*boldFont: xft:Terminus:bold:size=24,xft:Symbola:pixelsize=19


URxvt.saveLines: 8192
URxvt.scrollBar: false
URxvt.scrollstyle: plainp
URxvt.rightScrollBar: false
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
URxvt.visualBell: false
URxvt.perl-ext-common: default,clipboard,resize-font
URxvt*dynamicColors: on
URxvt.url-launcher: firefox
URxvt.matcher.button: 3
URxvt.iso14755_52: false
URxvt.iso14755: false
URxvt.clipboard.autocopy: true
URxvt.keysym.C-M-c: perl:clipboard:copy
URxvt.keysym.C-M-v: perl:clipboard:paste

! URxvt.background:  #1c1b1a
! URxvt.foreground:  #f8f6f2
URxvt.cursorColor: #aeee00
URxvt.scrollColor: #45413b


! URxvt.keysym.C-minus:     font-size:decrease
! URxvt.keysym.C-plus:   font-size:increase
! URxvt.keysym.C-equal:  font-size:reset
! URxvt.keysym.C-question:  font-size:show


URxvt.resize-font.smaller: C-minus
URxvt.resize-font.bigger: C-plus
URxvt.resize-font.reset: C-equal
URxvt.resize-font.show: C-question


!font settings                          -*-terminus-*-*-*-*-32-*-*-*-*-*-*-*
xscreensaver.Dialog.headingFont:        -*-terminus-*-*-*-32-*-*-*-*-*-*-*
xscreensaver.Dialog.bodyFont:           -*-terminus-bold-*-*-*-32-*-*-*-*-*-*-*
xscreensaver.Dialog.labelFont:          -*-terminus-*-*-*-32-*-*-*-*-*-*-*
xscreensaver.Dialog.unameFont:          -*-terminus-*-*-*-32-*-*-*-*-*-*-*
xscreensaver.Dialog.buttonFont:         -*-terminus-bold-*-*-*-32-*-*-*-*-*-*-*
xscreensaver.Dialog.dateFont:           -*-terminus-bold-*-*-*-16-*-*-*-*-*-*-*
xscreensaver.passwd.passwdFont:         -*-terminus-bold-*-*-*-32-*-*-*-*-*-*-*

! xmessage*form.okay.shapeStyle: rectangle
xmessage*form.okay.background: #1c1b1a
xmessage*form.okay.foreground: #d9cec3
xmessage*message*background: #1c1b1a
xmessage*background: #1c1b1a
xmessage*foreground: #d9cec3
! xmessage*Scrollbar.width: 10
! xmessage*Scrollbar.borderWidth: 1
xmessage*Text.?.cursorColor: #aeee00
xmessage*Text.borderColor: #d9cec3
xmessage*Text*background: #1c1b1a
! xmessage*Command.highlightThickness: 1
! xmessage*Command.internalWidth: 5
! xmessage*Command.internalHeight: 3
! xmessage*Command.borderColor: gray40
! xmessage*Command.shapeStyle: Rectangle
! xmessage*Command.background: gray80
! xmessage*faceName: DejaVuSansMono
! xmessage*faceSize: 12
xmessage*message.scrollVertical: Always
xmessage*message.scrollHorizontal: Never

!general dialog box (affects main hostname, username, password text)
! xscreensaver.Dialog.foreground:         #00efef
! xscreensaver.Dialog.background:         #111111
! xscreensaver.Dialog.topShadowColor:     #111111
! xscreensaver.Dialog.bottomShadowColor:  #111111
! xscreensaver.Dialog.Button.foreground:  #111111
! xscreensaver.Dialog.Button.background:  #2040e0

!username/password input box and date text colour
! xscreensaver.Dialog.text.foreground:    #2040e0
! xscreensaver.Dialog.text.background:    #efefef
! xscreensaver.Dialog.internalBorderWidth:36
! xscreensaver.Dialog.borderWidth:        0
! xscreensaver.Dialog.shadowThickness:    2

!timeout bar (background is actually determined by Dialog.text.background)
! xscreensaver.passwd.thermometer.foreground:  #2040e0
! xscreensaver.passwd.thermometer.background:  #2040e0
! xscreensaver.passwd.thermometer.width:       0

!datestamp format--see the strftime(3) manual page for details
! xscreensaver.dateFormat:    %I:%M%p - %a %b %d, %Y



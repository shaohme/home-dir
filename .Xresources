Xcursor.theme: Vanilla-DMZ
Xcursor.size: 48
! Xft.antialias: 1
! Xft.autohint: true
! Xft.hinting: 1
! Xft.hintstyle: hintslight
! Xft.rgba: rgb
! Xft.lcdfilter: lcddefault

*font: -*-*-*-*-*-*-28-*-*-*-*-*-*-*

! URxvt*font: -*-terminus-bold-*-*-*-28-*-*-*-*-*-*-*
! XTerm*font: -*-terminus-bold-*-*-*-28-*-*-*-*-*-*-*

!! Defining italic and bold fonts because they look like crap
!! in X with terminus font
! *boldFont: -*-terminus-bold-*-*-*-28-*-*-*-*-*-*-*
! *italicFont: -*-terminus-bold-*-*-*-28-*-*-*-*-*-*-*
! *bolditalicFont: -*-terminus-bold-*-*-*-28-*-*-*-*-*-*-*
URxvt*font:     xft:Monospace:size=12:antialias=true,xft:Symbola:pixelsize=19:antialias=true
URxvt*boldFont: xft:Monospace-bold:bold:size=12:antialias=true,xft:Symbola:pixelsize=19:antialias=true
! URxvt*font:     xft:Terminus:bold:size=26,xft:Symbola:pixelsize=19
! URxvt*boldFont: xft:Terminus:bold:size=24,xft:Symbola:pixelsize=19


URxvt.saveLines: 8192
URxvt.scrollBar: false
URxvt.scrollstyle: plainp
URxvt.rightScrollBar: false
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
URxvt.visualBell: false
URxvt.perl-ext-common: default,clipboard,resize-font
URxvt*dynamicColors: on
URxvt.url-launcher: firefox
URxvt.matcher.button: 3
URxvt.iso14755_52: false
URxvt.iso14755: false
URxvt.clipboard.autocopy: true
URxvt.keysym.C-M-c: perl:clipboard:copy
URxvt.keysym.C-M-v: perl:clipboard:paste

URxvt.resize-font.smaller: C-minus
URxvt.resize-font.bigger: C-plus
URxvt.resize-font.reset: C-equal
URxvt.resize-font.show: C-question

